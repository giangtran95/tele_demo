generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id            Int          @id @default(autoincrement())
  tele_id       String?      @unique @db.VarChar
  tele_username String?      @db.VarChar
  status        tele_status?
  created_at    DateTime?    @db.Timestamp(6)
}

enum tele_status {
  pending
  activate
}
